<?xml version="1.0"?>
<bodypart>
    <priority>500</priority>
    <fallback>1</fallback>
    <mask_calculation_mode>combine_with_parent_mask</mask_calculation_mode>
    <conditions>
        <condition>
            <breast_size>flat;almost unnoticeable;extremely tiny</breast_size>
        </condition>
    </conditions>
    <connections_parent>
        <connection>
            <point_1>7;62</point_1>
            <point_2>347;62</point_2>
        </connection>
    </connections_parent>
    <positions>
        <position id="nipple_left">
            <type>nipple</type>
            <draw_mode>use_parent_parent_alpha</draw_mode>
            <modifiers>
                <scale>0.8</scale>
            </modifiers>
            <multiple>
                <rows_count_param>nipple_count_per_breast</rows_count_param>
                <output_mode>line</output_mode>
                <max_distance>90;110</max_distance>
                <offset>10;20</offset>
                <secondary_priority_inc>-0.01</secondary_priority_inc>
            </multiple>
            <point>90;116</point>
        </position>
        <position id="nipple_right">
            <type>nipple</type>
            <draw_mode>use_parent_parent_alpha</draw_mode>
            <modifiers>
                <scale>0.8</scale>
                <inverse_x>1</inverse_x>
            </modifiers>
            <multiple>
                <rows_count_param>nipple_count_per_breast</rows_count_param>
                <output_mode>line</output_mode>
                <max_distance>-90;110</max_distance>
                <offset>-10;20</offset>
                <secondary_priority_inc>-0.01</secondary_priority_inc>
            </multiple>
            <point>263;116</point>
        </position>
    </positions>
</bodypart>