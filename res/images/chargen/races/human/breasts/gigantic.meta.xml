<?xml version="1.0"?>
<bodypart>
    <priority>200</priority>
    <conditions>
        <condition>
            <breast_size>gigantic;colossal;extreme;monstrous;hyper</breast_size>
        </condition>
    </conditions>
    <mod_conditions>
        <condition scale_y="1.1">
            <breast_size>colossal</breast_size>
        </condition>
        <condition scale_y="1.3" scale_x="1.05">
            <breast_size>extreme</breast_size>
        </condition>
        <condition scale_y="1.5" scale_x="1.1">
            <breast_size>monstrous</breast_size>
        </condition>
        <condition scale_y="1.7" scale_x="1.25">
            <breast_size>hyper</breast_size>
        </condition>
    </mod_conditions>
    <connections_parent>
        <connection>
            <point_1>198;48</point_1>
            <point_2>526;48</point_2>
        </connection>
    </connections_parent>
    <positions>
        <position id="nipple_left">
            <type>nipple</type>
            <draw_mode>use_parent_alpha</draw_mode>
            <multiple>
                <rows_count_param>nipple_count_per_breast</rows_count_param>
                <output_mode>polygon</output_mode>
                <max_distance>180</max_distance>
                <mid_distance>120</mid_distance>
                <min_distance>10%</min_distance>
                <secondary_priority_inc>-0.01</secondary_priority_inc>
            </multiple>
            <point>160;320</point>
        </position>
        <position id="nipple_right">
            <type>nipple</type>
            <draw_mode>use_parent_alpha</draw_mode>
            <multiple>
                <rows_count_param>nipple_count_per_breast</rows_count_param>
                <output_mode>polygon</output_mode>
                <max_distance>180</max_distance>
                <mid_distance>120</mid_distance>
                <min_distance>10%</min_distance>
                <secondary_priority_inc>-0.01</secondary_priority_inc>
            </multiple>
            <modifiers>
                <inverse_x>1</inverse_x>
            </modifiers>
            <point>560;320</point>
        </position>
    </positions>
</bodypart>